{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "additionalProperties": false,
    "properties": {
        "ability_icon": {
            "type": "string"
        },
        "active": {
            "type": "string"
        },
        "active_values": {
            "patternProperties": {
                "^.*$": {
                    "type": "number"
                }
            },
            "type": [
                "object",
                "null"
            ]
        },
        "icon": {
            "type": "string"
        },
        "name": {
            "type": "string"
        },
        "passive": {
            "type": "string"
        },
        "passive_values": {
            "patternProperties": {
                "^.*$": {
                    "type": "number"
                }
            },
            "type": [
                "object",
                "null"
            ]
        }
    },
    "required": [
        "name",
        "passive",
        "active"
    ],
    "title": "Dauntless-Builder Lantern Schema",
    "type": "object"
}
