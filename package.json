{
    "name": "dauntless-builder",
    "version": "2.0.0",
    "type": "module",
    "repository": "git@github.com:atomicptr/dauntless-builder",
    "author": "Christopher Kaster <me@atomicptr.de>",
    "license": "AGPLv3",
    "private": true,
    "scripts": {
        "build": "yarn build:deps && yarn build:frontend",
        "build:data": "node scripts/build-data.js",
        "build:i18n": "node scripts/build-i18n.js",
        "build:about": "ts-node scripts/build-about.ts",
        "build:deps": "yarn build:data && yarn build:i18n && yarn build:about && yarn build:sitemap",
        "build:metabuilds": "node scripts/build-meta-builds.js",
        "build:sitemap": "ts-node scripts/build-sitemap.ts",
        "build:frontend": "vite build",
        "dev": "vite dev",
        "test": "node --experimental-vm-modules ./node_modules/.bin/jest",
        "deploy": "yarn build && ./scripts/deploy.sh",
        "lint": "jsonsort -i 4 -c src/translations/*.json .eslintrc tsconfig.json data/_schemas/**.json && eslint .",
        "lint:fix": "prettier . --write && jsonsort -i 4 src/translations/*.json .eslintrc tsconfig.json data/_schemas/**.json && eslint . --fix",
        "check:missing-translations": "node ./scripts/check-missing-translations.js"
    },
    "engines": {
        "node": ">=16.0.0",
        "npm": "please-use-yarn",
        "yarn": ">=1.20.0"
    },
    "dependencies": {
        "@emotion/react": "^11.9.3",
        "@emotion/styled": "^11.9.3",
        "@mui/icons-material": "^5.8.4",
        "@mui/material": "^5.8.4",
        "@reduxjs/toolkit": "^1.8.2",
        "hashids": "^2.2.10",
        "i18next": "^21.8.10",
        "i18next-browser-languagedetector": "^6.1.4",
        "md5": "^2.3.0",
        "notistack": "^2.0.5",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-i18next": "^11.17.2",
        "react-icons": "^4.4.0",
        "react-lazy-load-image-component": "^1.5.4",
        "react-redux": "^8.0.2",
        "react-router-dom": "^6.3.0",
        "react-virtualized-auto-sizer": "^1.0.6",
        "react-window": "^1.8.7",
        "translation-check": "^1.0.2",
        "ts-pattern": "^4.0.3"
    },
    "devDependencies": {
        "@types/jest": "^28.1.2",
        "@types/node": "^18.0.0",
        "@types/react": "^18.0.14",
        "@types/react-dom": "^18.0.5",
        "@types/react-helmet": "^6.1.5",
        "@types/react-lazy-load-image-component": "^1.5.2",
        "@types/react-redux": "^7.1.24",
        "@types/react-virtualized-auto-sizer": "^1.0.1",
        "@types/react-window": "^1.8.5",
        "@typescript-eslint/eslint-plugin": "^5.29.0",
        "@typescript-eslint/parser": "^5.29.0",
        "@vitejs/plugin-react": "^1.3.2",
        "ajv": "^8.11.0",
        "axios": "^0.27.2",
        "eslint": "^8.18.0",
        "eslint-plugin-i18next": "^6.0.0-4",
        "eslint-plugin-react": "^7.30.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "eslint-plugin-simple-import-sort": "^7.0.0",
        "eslint-plugin-sort-keys-fix": "^1.1.2",
        "eslint-plugin-unused-imports": "^2.0.0",
        "glob": "^8.0.3",
        "jest": "^28.1.1",
        "jest-environment-jsdom": "^28.1.1",
        "js-yaml": "^4.1.0",
        "json-sort-cli": "^2.0.14",
        "license-checker": "^25.0.1",
        "ts-jest": "^28.0.5",
        "ts-node": "^10.8.1",
        "typescript": "^4.7.4",
        "vite": "^2.9.12",
        "vite-plugin-favicon2": "^1.1.4",
        "vite-plugin-pwa": "^0.12.0"
    }
}
